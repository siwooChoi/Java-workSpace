package Panels;

import java.awt.Graphics;
import java.awt.Image;

import javax.swing.ImageIcon;
import javax.swing.JButton;

import interfaces.FramePanelSetting;

public class Buttons extends JButton implements FramePanelSetting{
	private String imgName;
	private ImageIcon i;
	
	public Buttons(String argImgName){
		
		this.i = new ImageIcon(argImgName);
		setBounds(100, 350, VALUE_BTN_WIDTH, VALUE_BTN_HEIGHT);
		
	}

	
	
//	ImageIcon i = new ImageIcon(imgName);
	public void paintComponent(Graphics g) {
		g.drawImage(this.i.getImage(), 0, 0, VALUE_BTN_WIDTH, VALUE_BTN_HEIGHT, null);
	}
	
	
	
	
	

}
