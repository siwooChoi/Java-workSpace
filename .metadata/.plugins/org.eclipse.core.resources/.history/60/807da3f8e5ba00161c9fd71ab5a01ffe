package cyka2;

class A{
	public static void main(String[] args){

		for (int i = 0; i < 5; i++) {

			   for(int j = 0; j < 10; j++) {

			      if(i <= j || j <= 9 - i) {
			         System.out.print("*");
			      } else {

			         System.out.print(" ");
			      }
			   }
			   System.out.println();
			}

  }
}
