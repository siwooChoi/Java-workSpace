package Panels;

import java.awt.Graphics;
import java.awt.Image;

import javax.swing.ImageIcon;
import javax.swing.JButton;

import interfaces.FramePanelSetting;

public class Buttons extends JButton implements FramePanelSetting{
	private String imgName;
	
	public Buttons(String argImgName){
		ImageIcon i = new ImageIcon(argImgName);
	}


	
//	ImageIcon i = new ImageIcon(imgName);
	
	public void paintComponent(Graphics g) {
		g.drawImage(i.getImage(), 0, 0, VALUE_BTN_WIDTH, VALUE_BTN_HEIGHT, null);
	}
	
	
	

}
